<div id="<%= dom_id profile %>">
  <p>
    <strong>Name:</strong>
    <%= profile.name %>
  </p>

  <p>
    <strong>Youtube:</strong>
    <%= link_to profile.youtube, profile.youtube %>
  </p>

  <p>
    <strong>Twitter:</strong>
    <%= profile.twitter %>
  </p>

  <p>
    <strong>Other:</strong>
    <%= profile.other %>
  </p>

</div>
